        "for epoch in range(10):\n",
        "  ypred = w1*x1 + w2*x2 +b\n",
        "  error = (y- ypred)**2\n",
        "  dEw1 = 2*(y-ypred)*(-x1)\n",
        "  dEw2 = 2*(y-ypred)*(-x2)\n",
        "  dEb = 2*(y-ypred)*(-1)\n",
        "  w1 = w1 - lr*dEw1\n",
        "  w2 = w2 - lr*dEw2\n",
        "  b = b-lr*dEb\n",
        "  print(w1)\n",
        "  print(w2)\n",
        "  print(b)\n",
        "  print(error)\n",
